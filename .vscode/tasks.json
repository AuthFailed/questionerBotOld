{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build Release НЦК",
      "command": "dotnet",
      "type": "shell",
      "args": [
        "build",
        "${workspaceFolder}/QuestionBot.csproj",
        "/p:DefineConstants=НЦК",
        "/p:OutputPath=${workspaceFolder}/bin/nckquestion"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "Build Debug",
      "command": "dotnet",
      "type": "shell",
      "args": [
        "build",
        "${workspaceFolder}/QuestionBot.csproj",
        "/p:DefineConstants=DEBUG",
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "Build Debug НЦК",
      "command": "dotnet",
      "type": "shell",
      "args": [
        "build",
        "${workspaceFolder}/QuestionBot.csproj",
        "/p:DefineConstants=DEBUG",
        "/p:DefineConstants=НЦК",
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
  ]
}